{
    "plugins": [
      "jest"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
				"no-param-reassign": 0,
        "no-restricted-syntax": 0
    }
}
